rderService.post(data)\r\n        .then(response => {\r\n          console.log(response.data);\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    }\r\n    \r\n  },\r\n  mounted() {\r\n    this.retrieveArticles();\r\n  }\r\n};\r\n</script>\r\n\r\n"]},"metadata":{},"sourceType":"module"}