ound modal-default-button\"\r\n                    @click=\"classicModalHide\"\r\n                  >\r\n                    <md-icon>clear</md-icon>\r\n                  </md-button>\r\n                </template>\r\n\r\n                <template slot=\"body\">\r\n                  <p>\r\n                    Votre article a été ajouté avec succès\r\n                  </p>\r\n                </template>\r\n\r\n                <template slot=\"footer\">\r\n                  <md-button\r\n                    class=\"md-danger md-simple\"\r\n                    @click=\"classicModalHide\"\r\n                    >Close</md-button\r\n                  >\r\n                </template>\r\n              </modal>  \r\n\r\n\r\n<hr>\r\n\r\n</tr>\r\n</center>\r\n\r\n\r\n</div>\r\n\r\n          \r\n        </div>\r\n      </div>\r\n          </div>\r\n    </div>\r\n  </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n\r\n\r\n<script>\r\nimport ArticleService from \"@/services/ArticleService\";\r\nimport CartService from \"@/services/CartService\";\r\nimport { Modal } from \"@/components\";\r\n\r\nvar cart = [];\r\n \r\nimport JQuery from 'jquery'\r\nwindow.$ = JQuery\r\n// import { Component, Prop, Vue } from 'vue-property-decorator';\r\n\r\n// @Component\r\n// export default class ArticleList extends Vue {};\r\nexport default {\r\n                   classicModal: false,\r\n\r\n  components: {\r\n    Modal\r\n  },\r\n  bodyClass: \"landing-page\",\r\n  props: {\r\n    header: {\r\n      type: String,\r\n      default: require(\"@/assets/img/bghome.jpg\")\r\n    },\r\n    resto: {\r\n      type: String,\r\n      default: require(\"@/assets/img/resto.png\")\r\n    },\r\n    client: {\r\n      type: String,\r\n      default: require(\"@/assets/img/client.jpg\")\r\n    },\r\n    livreur: {\r\n      type: String,\r\n      default: require(\"@/assets/img/livreur.jpg\")\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n               classicModal: false,\r\n\r\n      Articles: [],\r\n      currentTutorial: null,\r\n      currentIndex: -1,\r\n      title: \"\"\r\n    };\r\n  },\r\n  computed: {\r\n    headerStyle() {\r\n      return {\r\n        backgroundImage: `url(${this.header})`\r\n      };\r\n    }\r\n  },\r\n  methods: {\r\n    classicModalHide() {\r\n      this.classicModal = false;\r\n    },\r\n    retrieveArticles() {\r\n      ArticleService.getAll()\r\n        .then(response => {\r\n          this.Articles = response.data;\r\n          console.log(response.data);\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    \r\n    },\r\n    AddToCart(id){\r\n      var cartCookie = CartService.getCookie('testuser');\r\n      \r\n      cart.push(id);\r\n      if (cartCookie == null) { \r\n        cartCookie = CartService.setCookie('testuser',cart,7);\r\n      }\r\n      else {\r\n        cart.push(cartCookie[0]);\r\n        cartCookie = CartService.setCookie('testuser',cart,7);\r\n      }\r\n      \r\n    },\r\n    \r\n  },\r\n  mounted() {\r\n    this.retrieveArticles();\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n.md-card-actions.text-center {\r\n  display: flex;\r\n  justify-content: center !important;\r\n}\r\n.contact-form {\r\n  margin-top: 30px;\r\n}\r\n\r\n.md-has-textarea + .md-layout {\r\n  margin-top: 15px;\r\n}\r\n</style>\r\n<style>\r\n.menu{\r\n    padding-top: 20px;\r\n    box-shadow: rgba(99, 99, 99, 0.2) 0px 2px 8px 0px;\r\n\r\n}\r\n.menutr{\r\n    background-color: white;\r\n    padding-top:20px;\r\n    box-shadow: rgba(0, 0, 0, 0.05) 0px 6px 24px 0px, rgba(0, 0, 0, 0.08) 0px 0px 0px 1px;\r\n    \r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}