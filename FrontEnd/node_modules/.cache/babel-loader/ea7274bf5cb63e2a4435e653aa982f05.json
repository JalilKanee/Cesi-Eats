-- Start Content -->\r\n <div class=\"bgmenu\">\r\n\r\n<div id=\"home\" class=\"w3-content\" style=\"width:600px;\">\r\n<center><h1 style=\"font-size: 45px;\r\n    font-family: UberMove, sans-serif;\r\n    font-weight: bolder;\r\n    text-shadow: 5px 5px 5px white;color:black;width: 400px;\">Tableau d'Ã©dition des articles</h1></center>\r\n<!-- Menu -->\r\n<div id=\"menu\" class=\"w3-container w3-black w3-xxlarge w3-padding-60\">\r\n\r\n<div class=\"w3-row w3-center w3-border w3-border-dark-grey\">\r\n<div><strong><p style=\"padding-top:20px;\">Les articles du menu</p></strong></div>\r\n</div>\r\n\r\n<div id=\"pizza\" class=\"w3-container w3-white w3-padding-32\">\r\n  <center>\r\n<tr  :class=\"{ active: index == currentIndex }\"\r\n          v-for=\"(Article, index) in Articles\"\r\n          :key=\"index\">\r\n<p style=\"font-size:30px;\"><b>{{Article.name}}</b> <span class=\"w3-right w3-tag w3-dark-grey w3-round\">{{Article.price}} DA</span>\r\n<div>\r\n    <img class=\"img-fluid\" style=\"width:150px;height:150px;\" :src=\"'http://127.0.0.1:4000\\\\'+Article.picture.path\" alt=\"Card image cap\">\r\n</div>\r\n<div>\r\n  \r\n<button  @click=\"AddToCart(Article._id)\"  class=\"btn btn-md mx-1 btn-success\" >Ajouter au panier</button>\r\n</div>\r\n</p>\r\n\r\n<hr>\r\n\r\n</tr>\r\n</center>\r\n\r\n\r\n</div>\r\n</div>\r\n</div>\r\n<!-- End Content -->\r\n</div>\r\n</div></div>\r\n\r\n  <!-- popup for added to cart -->\r\n  <div class=\"modal fade\" id=\"addToCartEffect\" tabindex=\"-1\"  aria-hidden=\"true\">\r\n    <div class=\"modal-dialog\">\r\n        \r\n        <i class=\"bi h1 text-success bi-bag-check-fill\"></i>\r\n          \r\n    </div>\r\n  </div>\r\n\r\n    </div> \r\n\r\n\r\n</template>\r\n<style>\r\n@import '@/assets/css/Navbar.css';\r\n@import '@/assets/css/AddMenu.css';\r\n</style>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n<script>\r\n\r\n\r\n\r\nimport ArticleService from \"@/services/ArticleService\";\r\nimport CartService from \"@/services/CartService\";\r\n\r\nvar cart = [];\r\n \r\nimport JQuery from 'jquery'\r\nwindow.$ = JQuery\r\n// import { Component, Prop, Vue } from 'vue-property-decorator';\r\n\r\n// @Component\r\n// export default class ArticleList extends Vue {};\r\nexport default {\r\n  name: \"Articlelist\",\r\n  data() {\r\n    return {\r\n      Articles: [],\r\n      currentTutorial: null,\r\n      currentIndex: -1,\r\n      title: \"\"\r\n    };\r\n  },\r\n  methods: {\r\n    retrieveArticles() {\r\n      ArticleService.getAll()\r\n        .then(response => {\r\n          this.Articles = response.data;\r\n          console.log(response.data);\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    \r\n    },\r\n    AddToCart(id){\r\n      var cartCookie = CartService.getCookie('testuser');\r\n      \r\n      cart.push(id);\r\n      if (cartCookie == null) { \r\n        cartCookie = CartService.setCookie('testuser',cart,7);\r\n      }\r\n      else {\r\n        cart.push(cartCookie[0]);\r\n        cartCookie = CartService.setCookie('testuser',cart,7);\r\n      }\r\n      $('#addToCartEffect').modal('toggle');\r\n      setTimeout(function() {$('#addToCartEffect').modal('hide');}, 500);\r\n    }\r\n    \r\n  },\r\n  mounted() {\r\n    this.retrieveArticles();\r\n  }\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}