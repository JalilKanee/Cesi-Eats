e.data);\n\n          _this.order.ids_article.push(response.data._id);\n\n          _this.order.total = +_this.order.total + +parseInt(response.data.price);\n          console.log(_this.order.ids_article);\n          console.log(_this.order.total);\n        }).catch(function (e) {\n          console.log(e);\n        });\n      }\n    },\n    saveorder: function saveorder() {\n      var data = {\n        payementMethod: this.order.payementMethod,\n        date: Date.now(),\n        ids_article: this.order.ids_article,\n        price: this.order.total,\n        id_restaurant: 'testidrestaurant',\n        id_client: 'testidclient'\n      };\n      var NotificationData = {\n        type_user: \"restaurateur\",\n        id_restaurant: 'testidrestaurant',\n        message: \"une Commande de hamster\"\n      };\n      OrderService.post(data).then(function (response) {\n        console.log(response.data);\n        $('#popupsuccess').modal('toggle');\n        NotificationService.post(NotificationData).then(function (response) {\n          console.log(response.data);\n        }).catch(function (e) {\n          console.log(e);\n        });\n      }).catch(function (e) {\n        console.log(e);\n      });\n    }\n  },\n  mounted: function mounted() {\n    this.retrieveArticles();\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmGA;AACA;AACA;AACA;AACA;AACAA;AACA;AAEA;EACAC,yBADA;EAEAC;IACAC;MACAC,YADA;MAEAC;IAFA,CADA;IAKAC;MACAF,YADA;MAEAC;IAFA,CALA;IASAE;MACAH,YADA;MAEAC;IAFA,CATA;IAaAG;MACAJ,YADA;MAEAC;IAFA;EAbA,CAFA;EAoBAI,IApBA,kBAoBA;IACA;MACAC,mBADA;MAGAC,YAHA;MAIAC;QACAC,QADA;QAEAC,eAFA;QAGAC,cAHA;QAIAC,QAJA;QAKAC,QALA;QAMAC,kBANA;QAOAC;MAPA,CAJA;MAaAC,qBAbA;MAcAC,gBAdA;MAeAC;IAfA;EAiBA,CAtCA;EAuCAC;IACAC,WADA,yBACA;MACA;QACAC;MADA;IAGA;EALA,CAvCA;EA8CAC;IACAC,gBADA,8BACA;MAAA;;MACA;QACA;QACAC,0BACAC,IADA,CACA;UACA;;UACA;;UACA;UACAC;UACAA;QACA,CAPA,EAQAC,KARA,CAQA;UACAD;QACA,CAVA