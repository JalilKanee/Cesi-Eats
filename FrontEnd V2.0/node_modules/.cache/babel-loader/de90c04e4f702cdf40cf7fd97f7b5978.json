ion()) {\n        axios.post(this.$hostname + \"/api/user/registration\", this.user).then(function (response) {\n          if (response.data.userId > 0) {\n            alert(\"Vous êtes maintenant inscrit(e)!\").then(function () {\n              _this.back();\n            });\n          } else {\n            alert(\"Error : Something went wrong.\");\n          }\n        }).catch(function (error) {\n          if (error.response) {\n            alert(error.response.data);\n          }\n        });\n      }\n    },\n    Toggle: function Toggle() {\n      var temp = document.getElementById(\"typepass\");\n      var temp2 = document.getElementById(\"typepass2\");\n\n      if (temp.type === \"password\" && temp2.type === \"password\") {\n        temp.type = \"text\";\n        temp2.type = \"text\";\n        document.getElementById(\"togglePassword\").className = \"bi bi-eye-slash\";\n      } else {\n        temp.type = \"password\";\n        temp2.type = \"password\";\n        document.getElementById(\"togglePassword\").className = \"bi bi-eye\";\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0IA;;AACA;AAEA;EACA;EACA;EACAA,IAHA,kBAGA;IAEA;MAEAC;QACAC,SADA;QAEAC,YAFA;QAGAC,SAHA;QAIAC,YAJA;QAKAC;MALA;IAFA;EAUA,CAfA;EAiBAC;IACAC,IADA,kBACA;MACA;QAAAC;MAAA;IACA,CAHA;IAIAC,eAJA,6BAIA;MACA;QACAC;QACA;MACA;;MACA;QACAA;QACA;MACA;;MACA;QACAA;QACA;MACA;;MACA;QACAA;QACA;MACA;;MACA;QACAA;QACA;MACA;;MACA;IAEA,CA3BA;IA4BAC,MA5BA,oBA4BA;MAAA;;MACA;QACAC,iEACAC,IADA,CACA;UACA;YACAH,0CACAG,IADA,CACA;cACA;YACA,CAHA;UAIA,CALA,MAKA;YACAH;UACA;QACA,CAVA,EAWAI,KAXA,CAWA;UACA;YACAJ;UACA;QACA,CAfA;MAgBA;IAEA,CAhDA;IAiDAK,MAjDA,oBAiDA;MACA;MACA;;MACA;QACAC;QACAC;QACAC;MAEA,CALA,MAMA;QACAF;QACAC;QACAC;MAEA;IACA;EAhEA;AAjBA","names":["data","user","userId","username","email","password","repeatPassword","methods","back","name","checkValidation","ale